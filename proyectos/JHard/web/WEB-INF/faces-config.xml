<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <view-handler>
            com.sun.rave.web.ui.appbase.faces.ViewHandlerImpl
        </view-handler>
    </application>
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>jhard.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>   
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>jhard.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>jhard.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jrequestUser</managed-bean-name>
        <managed-bean-class>jhard.jrequestUser</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jrequestUserSolicitud</managed-bean-name>
        <managed-bean-class>jhard.jrequestUserSolicitud</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/jrequestUser.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/jrequestUserSolicitud.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>jrequestAdmin</managed-bean-name>
        <managed-bean-class>jhard.jrequestAdmin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>JInventInstance</managed-bean-name>
        <managed-bean-class>edu.ues.jhard.beans.BeanBaseJInvent</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/jrequestAdmin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>case2</from-outcome>
            <to-view-id>/jrequestUserSolicitud.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>JHardminInstance</managed-bean-name>
        <managed-bean-class>edu.ues.jhard.beans.BeanBaseJHardmin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Redireccion</managed-bean-name>
        <managed-bean-class>edu.ues.jhard.util.Redireccion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jprocurUser</managed-bean-name>
        <managed-bean-class>jhard.jprocurUser</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Index</managed-bean-name>
        <managed-bean-class>jhard.Index</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jcanon</managed-bean-name>
        <managed-bean-class>jhard.jcanon</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>JRequestAdministracion</managed-bean-name>
        <managed-bean-class>jhard.JRequestAdministracion</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jcanonAdmin</managed-bean-name>
        <managed-bean-class>jhard.jcanonAdmin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jprocurAdmin</managed-bean-name>
        <managed-bean-class>jhard.jprocurAdmin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jwikiUser</managed-bean-name>
        <managed-bean-class>jhard.jwikiUser</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jprocurAdmin_1</managed-bean-name>
        <managed-bean-class>jhard.jprocurAdmin_1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jwikiAdmin</managed-bean-name>
        <managed-bean-class>jhard.jwikiAdmin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jmlHorario</managed-bean-name>
        <managed-bean-class>jhard.jmlHorario</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jmlHorarioAdmin</managed-bean-name>
        <managed-bean-class>jhard.jmlHorarioAdmin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jmlClase</managed-bean-name>
        <managed-bean-class>jhard.jmlClase</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>jmlIniciaClase</managed-bean-name>
        <managed-bean-class>jhard.jmlIniciaClase</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>BeanBaseJHard</managed-bean-name>
        <managed-bean-class>jhard.BeanBaseJHard</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>