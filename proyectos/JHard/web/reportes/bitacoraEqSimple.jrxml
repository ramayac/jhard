	<queryString>
		<![CDATA[SELECT
     estadoequipo.`nombre` AS estadoequipo_nombre,
     bitacoraestados.`descripcion` AS bitacoraestados_descripcion,
     bitacoraestados.`fecha` AS bitacoraestados_fecha,
     equiposimple.`descripcion` AS equiposimple_descripcion,
     equiposimple.`propietario` AS equiposimple_propietario
FROM
     `equiposimple` equiposimple INNER JOIN `bitacoraestados` bitacoraestados ON equiposimple.`idEquipoSimple` = bitacoraestados.`idequiposimple`
     INNER JOIN `estadoequipo` estadoequipo ON bitacoraestados.`idestado` = estadoequipo.`idestado`
     
WHERE
     `equiposimple`.idEquipoSimple = $P{idEquipoSimple}]]>
