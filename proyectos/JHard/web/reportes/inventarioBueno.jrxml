	<queryString>
		<![CDATA[SELECT
     existencia.`codigo` AS existencia_codigo,
     equipo.`nombre` AS equipo_nombre,
     equipo.`modelo` AS equipo_modelo,
     marca.`nombre` AS marca_nombre
FROM
     `equipo` equipo INNER JOIN `existencia` existencia ON equipo.`idequipo` = existencia.`idhardware`
     INNER JOIN `estadoequipo` estadoequipo ON existencia.`idestado` = estadoequipo.`idestado`
     INNER JOIN `marca` marca ON equipo.`idmarca` = marca.`idmarca`
