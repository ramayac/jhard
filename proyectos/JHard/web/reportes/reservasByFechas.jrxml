	<queryString>
		<![CDATA[SELECT
     docente.`apellidos` AS docente_apellidos,
     reserva.`fechahorainicioprestamo` AS reserva_fechahorainicioprestamo,
     equipo.`nombre` AS equipo_nombre,
     equipo.`modelo` AS equipo_modelo
FROM
     `docente` docente INNER JOIN `reserva` reserva ON docente.`iddocente` = reserva.`iddocente`
     INNER JOIN `existencia` existencia ON reserva.`idequipoexistente` = existencia.`idexistencia`
     INNER JOIN `equipo` equipo ON existencia.`idhardware` = equipo.`idequipo`
WHERE

reserva.`fechahorainicioprestamo` >= $P{fechaInicial}
AND
reserva.`fechahorainicioprestamo` <= $P{fechaFinal}

ORDER BY reserva.`fechahorainicioprestamo`]]>
	</queryString>
