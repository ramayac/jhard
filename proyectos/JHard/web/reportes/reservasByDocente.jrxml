	<parameter name="iddocente" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer (1)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     equipo.`nombre` AS equipo_nombre,
     equipo.`modelo` AS equipo_modelo,
     reserva.`fechahorainicioprestamo` AS reserva_fechahorainicioprestamo,
     docente.`apellidos` AS docente_apellidos,
     docente.`nombres` AS docente_nombres
FROM
     `existencia` existencia INNER JOIN `reserva` reserva ON existencia.`idexistencia` = reserva.`idequipoexistente`
     INNER JOIN `equipo` equipo ON existencia.`idhardware` = equipo.`idequipo`
     INNER JOIN `docente` docente ON reserva.`iddocente` = docente.`iddocente`
WHERE
     reserva.`iddocente` = $P{iddocente}]]>
	</queryString>
