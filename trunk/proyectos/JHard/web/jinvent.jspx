<jsp:root version="2.0" xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html" xmlns:ice="http://www.icesoft.com/icefaces/component" xmlns:jsp="http://java.sun.com/JSP/Page">

    <jsp:directive.page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"/>
   <f:view>
        <html id="outputHtml1">
            <head id="outputHead1">
                <!--ice:outputStyle href="./resources/stylesheet.css" id="outputStyle1"/-->
                <ice:outputStyle href="css/stylesheet.css" id="outputStyle1"/>
                <ice:outputStyle href="./xmlhttp/css/rime/rime.css" id="outputStyle2"/>
                <meta content="text/html; charset=utf-8" http-equiv="content-type"/>
                <title>.:: JHard ::.</title>
                <meta content="" name="keywords"/>
                <meta content="" name="description"/>
                <link href="css/default.css" rel="stylesheet" type="text/css"/>
                <link href="img/favicon.ico" rel="shortcut icon" type="image/x-icon"/>
                <script type="text/javascript" src="js/validaciones.js"></script>
            </head>
            <body id="outputBody1" style="-rave-layout: grid">
                <!--start header -->
                <div id="header">
                    <div id="menu">
                        <ul>
                            <li>
                                <a href="Index.iface">Principal</a>
                            </li>
                            <li>
                                <a href="jrequestUser.iface">Mantenimientos</a>
                            </li>
                            <li>
                                <a href="#">Grupos de Laboratorio</a>
                            </li>
                            <li class="current_page_item">
                                <a href="#">Inventario</a>
                            </li>
                            <li>
                                <a href="#">Wiki y Cursos</a>
                            </li>
                            <li class="last">
                                <a href="jcanon.iface">Cañones</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div id="logo">
                    <h1>Laboratorio de Hardware</h1>
                    <h2>Universidad de El Salvador, Facultad Multidisciplinaria de Occidente</h2>
                </div>
                <!-- end header -->
                <!-- start page -->
                <div id="page">
                    <!-- start content -->
                    <div id="content">
                        <div class="post">
                            <ice:form id="frmJInvent">
                            <h2 class="title">Administración del Inventario</h2>
                                <ice:panelGroup styleClass="entry" rendered="#{JHardminInstance.currentUser != null}">
                                    <ice:panelCollapsible expanded="true" rendered="#{JHardminInstance.currentUser.userRole.idrol == 1}" >
                                        <f:facet name="header">
                                            <h:outputFormat value="Elementos del inventario en la clasificación {0}">
                                                <f:param value="#{JInventInstance.currentClasificacion.nombre}" />
                                            </h:outputFormat>
                                        </f:facet>
                                        <ice:panelGroup id="divEquiposInventario" styleClass="frmElementList">
                                            <ice:panelDivider id="divTreeClasificaciones" orientation="vertical" style="width: 100%; height: 500px" dividerPosition="38">
                                                <f:facet name="first">
                                                    <ice:panelGroup >                                                    
                                                    <ice:tree id="treeClasificaciones" value="#{JInventInstance.clasificaciontm.modelo}" var="nodo" hideRootNode="false" hideNavigation="false" imageDir="./xmlhttp/css/rime/css-images/">
                                                        <ice:treeNode>
                                                            <f:facet name="icon">
                                                                <ice:panelGroup style="display: inline">
                                                                    <ice:graphicImage value="/xmlhttp/css/rime/css-images/#{nodo.userObject.icon}"/>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                            <f:facet name="content">
                                                                <ice:panelGroup style="display: inline" >
                                                                    <ice:commandLink actionListener="#{JInventInstance.clasificacionSelected}">
                                                                        <f:param name="clId" value="#{nodo.userObject.clasificacion.idclasificacion}" />
                                                                        <ice:outputText value="#{nodo.userObject.text}" styleClass="selectedNode#{nodo.userObject eq JInventInstance.clasificaciontm.currentUserObject} clasificacionEditable-#{nodo.userObject.clasificacion.idclasificacion > 16}" />
                                                                    </ice:commandLink>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                        </ice:treeNode>                                                        
                                                    </ice:tree>
                                                    <h3 class="tituloSeccion" style="margin-top: 30px;">Acciones de las clasificaciones</h3>
                                                    <ice:panelGroup styleClass="frmElementList">
                                                        <ul class="listaAcciones">                                                            
                                                        <li><ice:commandLink id="cmdAgregarSubclasificacion" value="Agregar subclasificacion" action="#{JInventInstance.crdClasificacion.showPopupAdd}" /></li>
                                                        <li><ice:commandLink id="cmdEditarClasificacion" value="Editar clasificacion seleccionada" action="#{JInventInstance.beforeShowPopupEditClasificacion}" /></li>
                                                        <li><ice:commandLink id="cmdEliminarClasificacion" value="Eliminar clasificacion seleccionada" action="#{JInventInstance.beforeShowPopupDelClasificacion}" /></li>
                                                        </ul>
                                                        <ice:panelPopup id="pupAddClasificacion" draggable="true" modal="true" rendered="#{JInventInstance.crdClasificacion.popupAddVisible}">
                                                            <f:facet name="header"><ice:outputText value="Agregar subclasificacion" /></f:facet>
                                                            <f:facet name="body">
                                                                <ice:panelGroup styleClass="frmElementList">
                                                                    <p>
                                                                        <ice:outputLabel id="lblNombreClasificacion" for="txtNombreClasificacion" value="Nombre:" />
                                                                        <ice:inputText id="txtNombreClasificacion" value="#{JInventInstance.nuevaClasificacion.nombre}" />
                                                                    </p>
                                                                    <p>
                                                                        <ice:outputLabel id="lblDescripcionClasificacion" for="txtDescripcionClasificacion" value="Descripcion:" />
                                                                        <ice:inputText id="txtDescripcionClasificacion" value="#{JInventInstance.nuevaClasificacion.descripcion}" />
                                                                    </p>
                                                                    <p class="actionSection">
                                                                        <ice:commandButton id="cmdCommitAgregarClasificacion" value="Agregar" action="#{JInventInstance.addClasificaicon}" />
                                                                        <ice:commandButton id="cmdCancelAgregarClasificacion" value="Cancelar" action="#{JInventInstance.crdClasificacion.hidePopupAdd}" />
                                                                    </p>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                        </ice:panelPopup>
                                                        <ice:panelPopup id="pupEditarClasificacion" draggable="true" modal="true" rendered="#{JInventInstance.crdClasificacion.popupEditVisible}">
                                                            <f:facet name="header"><ice:outputText value="Editar clasificacion" /></f:facet>
                                                                <f:facet name="body">
                                                                    <ice:panelGroup styleClass="frmElementList">
                                                                        <p>
                                                                            <ice:outputLabel id="lblNombreClasificacionEdit" for="txtNombreClasificacionEdit" value="Nombre:" />
                                                                            <ice:inputText id="txtNombreClasificacionEdit" value="#{JInventInstance.currentClasificacion.nombre}" />
                                                                        </p>
                                                                        <p>
                                                                            <ice:outputLabel id="lblDescripcionClasificacionEdit" for="txtDescripcionClasificacionEdit" value="Descripcion:" />
                                                                            <ice:inputText id="txtDescripcionClasificacionEdit" value="#{JInventInstance.currentClasificacion.descripcion}" />
                                                                        </p>
                                                                        <p class="actionSection">
                                                                            <ice:commandButton id="cmdCommitEditarClasificacion" value="Guardar cambios" action="#{JInventInstance.editClasificacion}" />
                                                                            <ice:commandButton id="cmdCancelEditarClasificaicon" value="Cancelar" action="#{JInventInstance.crdClasificacion.hidePopupEdit}" />
                                                                        </p>
                                                                    </ice:panelGroup>
                                                                </f:facet>
                                                        </ice:panelPopup>
                                                        <ice:panelPopup id="pupEliminarClasificacion" draggable="true" modal="true" rendered="#{JInventInstance.crdClasificacion.popupDelVisible}" >
                                                            <f:facet name="header"><ice:outputText value="Confirmacion" /></f:facet>
                                                            <f:facet name="body">
                                                                <ice:panelGroup styleClass="frmElementList">
                                                                    <p>
                                                                        <ice:outputText value="Esta seguro que desea eliminar la clasificacion seleccionada?" />
                                                                    </p>
                                                                    <p class="actionSection">
                                                                        <ice:commandButton id="cmdCommitEliminarClasificacion" value="Si" action="#{JInventInstance.delClasificacion}" />
                                                                        <ice:commandButton id="cmdCancelEliminarClasificacion" value="No" action="#{JInventInstance.crdClasificacion.hidePopupDel}" />
                                                                    </p>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                        </ice:panelPopup>
                                                    </ice:panelGroup>
                                                </ice:panelGroup>
                                                </f:facet>
                                                <f:facet name="second">
                                                    <ice:panelGroup>                                                        
                                                        <h3 class="tituloSeccion">
                                                            <ice:outputFormat value="Existencias de Equipos (Total: {0})">
                                                                <f:param value="#{JInventInstance.currentClasificacionExistenciaCollectionSize}"/>
                                                            </ice:outputFormat>
                                                            <ice:commandLink value="Agregar" action="#{JInventInstance.beforeAddExistencia}" styleClass="actionLink" />
                                                        </h3>
                                                        <ice:dataTable id="tblListaEquipos" rows="5" value="#{JInventInstance.currentClasificacion.existenciaCollection}" var="existencia" style="margin-bottom: 0px;" >
                                                        <ice:column>
                                                            <ice:graphicImage alt="#{existencia.idestado.nombre}" url="img/#{existencia.imagenEstado}"  />
                                                        </ice:column>
                                                        <ice:column>
                                                            <f:facet name="header"><ice:outputText value="Código" /></f:facet>
                                                            <ice:outputText value="#{existencia.codigo}" />
                                                        </ice:column>
                                                            <ice:column>
                                                                <f:facet name="header"><ice:outputText value="Nombre" /></f:facet>
                                                                <ice:outputText value="#{existencia.idhardware.nombre}" />
                                                            </ice:column>
                                                            <ice:column>
                                                                <f:facet name="header"><ice:outputText value="Marca" /></f:facet>
                                                                <ice:outputText value="#{existencia.idhardware.idmarca.nombre}" />
                                                            </ice:column>                                                            
                                                            <ice:column>
                                                                <f:facet name="header"><ice:outputText value="Modelo" /></f:facet>
                                                                <ice:outputText value="#{existencia.idhardware.modelo}" />
                                                            </ice:column>
                                                            <ice:column>
                                                                <f:facet name="header"><ice:outputText value="Ubicación" /></f:facet>
                                                                <ice:outputText value="#{existencia.idubicacion.nombre}" />
                                                            </ice:column>
                                                            <ice:column>
                                                                <ice:commandLink value="Editar" action="#{JInventInstance.editExistencia}">
                                                                    <f:param name="currentId" value="#{existencia.idexistencia}" />
                                                                </ice:commandLink>
                                                            </ice:column>
                                                            <ice:column>
                                                                <ice:commandLink value="Eliminar" action="#{JInventInstance.crdExistencia.showPopupDel}">
                                                                    <f:param name="currentId" value="#{existencia.idexistencia}" />
                                                                </ice:commandLink>
                                                            </ice:column>
                                                        </ice:dataTable>
                                                        <ice:dataPaginator id="pgrListaEquipos" for="tblListaEquipos" paginatorMaxPages="4" paginator="true" style="margin: 0px 10px 10px" rendered="#{JInventInstance.sizeListaExistencias > 5}">
                                                            <f:facet name="first"><ice:graphicImage url="./xmlhttp/css/rime/css-images/arrow-first.gif" style="border:none;" title="First Page"/></f:facet>
                                                            <f:facet name="previous"><ice:graphicImage url="./xmlhttp/css/rime/css-images/arrow-previous.gif" style="border:none;" title="Prev Page"/></f:facet>
                                                            <f:facet name="next"><ice:graphicImage url="./xmlhttp/css/rime/css-images/arrow-next.gif" style="border:none;" title="Next Page"/></f:facet>
                                                            <f:facet name="last"><ice:graphicImage url="./xmlhttp/css/rime/css-images/arrow-last.gif" style="border:none;" title="Last Page"/></f:facet>
                                                        </ice:dataPaginator>
                                                        <ice:panelPopup id="pupAgregarExistencia" modal="true" draggable="true" rendered="#{JInventInstance.crdExistencia.popupAddVisible or JInventInstance.crdExistencia.popupEditVisible}" >
                                                            <f:facet name="header"><ice:panelGroup><ice:outputText value="Agregar nueva existencia" rendered="#{!JInventInstance.existenciaEditMode}" /><ice:outputText value="Editar existencia" rendered="#{JInventInstance.existenciaEditMode}" /></ice:panelGroup></f:facet>
                                                            <f:facet name="body">
                                                                <ice:panelGroup styleClass="frmElementList" style="background-color: #FCFCFC;">
                                                                    <p>
                                                                        <ice:outputLabel value="Asociada al equipo:" for="cmbEquipos" />
                                                                        <ice:selectOneListbox id="cmbEquipos" value="#{JInventInstance.equipoSelected}" size="1">
                                                                            <f:selectItems value="#{JInventInstance.listaItemsEquipos}" />
                                                                        </ice:selectOneListbox>
                                                                    </p>
                                                                    <p>
                                                                        <ice:outputLabel id="lblCodigoExistencia" for="txtCodigoExistencia" value="Código:" />
                                                                        <ice:inputText id="txtCodigoExistencia" value="#{JInventInstance.currentExistencia.codigo}" />
                                                                    </p>
                                                                    <p>
                                                                        <ice:outputLabel id="lblUbicacionExistencia" for="cmbUbicacionExistencia" value="Ubicación:" />
                                                                        <ice:selectOneListbox id="cmbUbicacionExistencia" size="1" value="#{JInventInstance.ubicacionSelected}">
                                                                            <f:selectItems value="#{JInventInstance.listaItemsUbicaciones}" />
                                                                        </ice:selectOneListbox>
                                                                    </p>
                                                                    <p>
                                                                        <ice:panelTabSet style="margin-top: 30px;">
                                                                        <ice:panelTab id="tabSoftwareExistencia" label="Software instalado">
                                                                            <ice:dataTable id="tblSoftwareExistencia" value="#{JInventInstance.currentExistencia.instalacionCollection}" var="instalacion">
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Nombre" /></f:facet>
                                                                                    <ice:outputText value="#{instalacion.idsoftware.nombre}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Versión" /></f:facet>
                                                                                    <ice:outputText value="#{instalacion.idsoftware.version}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Fecha de instalación" /></f:facet>
                                                                                    <ice:outputText value="#{instalacion.fechainstalacion}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <ice:commandLink value="Eliminar" action="#{JInventInstance.eliminarInstalacionExistencia}">
                                                                                        <f:param name="currentId" value="#{instalacion.idinstalacion}" />
                                                                                    </ice:commandLink>
                                                                                </ice:column>
                                                                            </ice:dataTable>
                                                                            <ice:selectOneListbox id="cmbListaSoftwareDisponible" size="1" value="#{JInventInstance.softwareSelected}">
                                                                                <f:selectItems value="#{JInventInstance.listaItemsSoftware}" />
                                                                            </ice:selectOneListbox>
                                                                            <ice:commandLink value="Agregar" action="#{JInventInstance.agregarInstalacionExistencia}" />
                                                                        </ice:panelTab>
                                                                        <ice:panelTab id="tabAccesorioExistencia" label="Accesorios">
                                                                            <ice:dataTable id="tblAccesorioExistencia" value="#{JInventInstance.currentExistencia.accesorioCollection}" var="accesorio" >
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Nombre" /></f:facet>
                                                                                    <ice:outputText value="#{accesorio.nombre}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Marca" /></f:facet>
                                                                                    <ice:outputText value="#{accesorio.idmarca.nombre}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Modelo" /></f:facet>
                                                                                    <ice:outputText value="#{accesorio.modelo}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <ice:commandLink value="Eliminar" action="#{JInventInstance.eliminarAccesorioExistencia}">
                                                                                        <f:param name="currentId" value="#{accesorio.idaccesorio}" />
                                                                                    </ice:commandLink>
                                                                                </ice:column>
                                                                            </ice:dataTable>
                                                                            <ice:panelGroup rendered="#{JInventInstance.listaTodosAccesoriosSize > 0}">
                                                                                <ice:selectOneListbox id="cmbListaAccesoriosDisponibles" size="1" value="#{JInventInstance.accesorioSelected}">
                                                                                    <f:selectItems value="#{JInventInstance.listaItemsAccesorios}" />
                                                                                </ice:selectOneListbox>
                                                                                <ice:commandLink value="Agregar" action="#{JInventInstance.agregarAccesorioExistencia}" />
                                                                            </ice:panelGroup>
                                                                        </ice:panelTab>
                                                                        <ice:panelTab id="tabPiezaExistencia" label="Piezas">
                                                                            <ice:dataTable id="tblPiezaExistencia" value="#{JInventInstance.currentExistencia.piezaCollection}" var="pieza">
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Nombre" /></f:facet>
                                                                                    <ice:outputText value="#{pieza.nombre}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Marca" /></f:facet>
                                                                                    <ice:outputText value="#{pieza.idmarca.nombre}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <f:facet name="header"><ice:outputText value="Modelo" /></f:facet>
                                                                                    <ice:outputText value="#{pieza.modelo}" />
                                                                                </ice:column>
                                                                                <ice:column>
                                                                                    <ice:commandLink value="Eliminar" action="#{JInventInstance.eliminarPiezaExistencia}">
                                                                                        <f:param name="currentId" value="#{pieza.idpieza}" />
                                                                                    </ice:commandLink>
                                                                                </ice:column>
                                                                            </ice:dataTable>
                                                                            <ice:panelGroup rendered="#{JInventInstance.listaTodasPiezasSize > 0}">
                                                                                <ice:selectOneListbox id="cmbListaPiezasDisponibles" size="1" value="#{JInventInstance.piezaSelected}">
                                                                                    <f:selectItems value="#{JInventInstance.listaItemsPiezas}" />
                                                                                </ice:selectOneListbox>
                                                                                <ice:commandLink value="Agregar" action="#{JInventInstance.agregarPiezaExistencia}" />
                                                                            </ice:panelGroup>
                                                                        </ice:panelTab>
                                                                    </ice:panelTabSet>
                                                                    </p>
                                                                    <p class="actionSection">
                                                                        <ice:commandButton id="cmdConfirmAgregarExistencia" value="Agregar" action="#{JInventInstance.addExistencia}" rendered="#{!JInventInstance.existenciaEditMode}" />
                                                                        <ice:commandButton id="cmdCancelAgregarExistencia" value="Cancelar" action="#{JInventInstance.cancelAddEditExistencia}" rendered="#{!JInventInstance.existenciaEditMode}" />

                                                                        <ice:commandButton id="cmdConfirmEditarExistencia" value="Guardar cambios" action="#{JInventInstance.commitEditExistencia}" rendered="#{JInventInstance.existenciaEditMode}" />
                                                                        <ice:commandButton id="cmdCancelEditarExistencia" value="Cancelar" action="#{JInventInstance.cancelAddEditExistencia}" rendered="#{JInventInstance.existenciaEditMode}" />
                                                                    </p>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                        </ice:panelPopup>
                                                        <ice:panelPopup id="pupEliminarExistencia" draggable="true" modal="true" rendered="#{JInventInstance.crdExistencia.popupDelVisible}">
                                                            <f:facet name="header"><ice:outputText value="Confirmación" /></f:facet>
                                                            <f:facet name="body">
                                                                <ice:panelGroup styleClass="frmElementList">
                                                                    <p>
                                                                        <ice:outputText value="Esta seguro que desea eliminar el elemento seleccionado?" />
                                                                    </p>
                                                                    <p class="actionSection">
                                                                        <ice:commandButton id="cmdConfirmDelExistencia" value="Si" action="#{JInventInstance.delExistencia}" />
                                                                        <ice:commandButton id="cmdCancelDelExistencia" value="No" action="#{JInventInstance.crdExistencia.hidePopupDel}" />
                                                                    </p>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                        </ice:panelPopup>
                                                        <ice:panelPopup id="pupEditarEquipo" draggable="true" modal="true" rendered="#{JInventInstance.crdEquipo.popupEditVisible}">
                                                            <f:facet name="header">
                                                                <ice:outputFormat value="Editar equipo {0}">
                                                                    <f:param value="#{JInventInstance.currentEquipo.nombre}" />
                                                                </ice:outputFormat>
                                                            </f:facet>
                                                            <f:facet name="body">
                                                                <ice:panelGroup styleClass="frmElementList">
                                                                    <p>
                                                                        <ice:outputLabel id="lblNombreEquipoEdit" for="txtNombreEquipoEdit" value="Nombre:" />
                                                                        <ice:inputText id="txtNombreEquipoEdit" value="#{JInventInstance.currentEquipo.nombre}" />
                                                                    </p>
                                                                    <p>
                                                                        <ice:outputLabel id="lblMarcaEquipoEdit" for="cmbMarcaEquipoEdit" value="Marca:" />
                                                                        <ice:selectOneListbox id="cmbMarcaEquipoEdit" size="1" value="#{JInventInstance.marcaSelected}">
                                                                            <f:selectItems value="#{JInventInstance.listaItemsMarcas}" />
                                                                            <f:selectItem itemLabel="Agregar nueva..." itemValue="-1" />
                                                                        </ice:selectOneListbox>
                                                                    </p>
                                                                    <p>
                                                                        <ice:outputLabel id="lblModeloEquipoEdit" for="txtModeloEquipoEdit" value="Modelo:" />
                                                                        <ice:inputText id="txtModeloEquipoEdit" value="#{JInventInstance.currentEquipo.modelo}" />
                                                                    </p>
                                                                    <p>
                                                                        <ice:outputLabel id="lblListaExistenciasEdit" for="tblExistenciasEdit" value="Existencias:" />
                                                                        <ice:dataTable id="tblExistenciasEdit" value="#{JInventInstance.currentEquipo.existenciaCollection}" var="existencia" >
                                                                            <ice:column>
                                                                                <ice:graphicImage alt="#{existencia.idestado.nombre}" url="img/#{existencia.imagenEstado}" />
                                                                            </ice:column>
                                                                            <ice:column>
                                                                                <f:facet name="header"><ice:outputText value="Código" /></f:facet>
                                                                                <ice:outputText value="#{existencia.codigo}" />
                                                                            </ice:column>
                                                                            <ice:column>
                                                                                <f:facet name="header"><ice:outputText value="Ubicación" /></f:facet>
                                                                                <ice:outputText value="#{existencia.idubicacion.nombre}" />
                                                                            </ice:column>
                                                                            
                                                                        </ice:dataTable>
                                                                    </p>
                                                                    <p class="actionSection">
                                                                        <ice:commandButton id="commitEditarEquipo" value="Guardar Cambios" action="#{JInventInstance.commitEditEquipo}" />
                                                                        <ice:commandButton id="cancelEditarEquipo" value="Cancelar" action="#{JInventInstance.cancelAddEditEquipo}" />
                                                                    </p>
                                                                </ice:panelGroup>
                                                            </f:facet>
                                                        </ice:panelPopup>
                                                        <ice:panelPopup id="pupConfirmEliminarEquipo" modal="true" draggable="true" title="Confirmación" rendered="#{JInventInstance.crdEquipo.popupDelVisible}">
                                                            <f:facet name="header"><ice:outputText value="Confirmación" /></f:facet>
                                                            <f:facet name="body">
                                                                <ice:panelGroup>
