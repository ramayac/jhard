	<parameter name="idcurso" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="idinstructor" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer (1)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    curso.`nombre` AS curso_nombre,
    instructor.`carnet` AS instructor_carnet,
    instructor.`apellidos` AS instructor_apellidos,
    instructor.`nombres` AS instructor_nombres,
    estudiante.`carnet` AS estudiante_carnet,
    estudiante.`apellidos` AS estudiante_apellidos,
    estudiante.`nombres` AS estudiante_nombres,
    materia.`nombre` AS materia_nombre
FROM
    `curso` curso INNER JOIN `inscripcion` inscripcion ON curso.`idcurso` = inscripcion.`idcurso`
    INNER JOIN `estudiante` estudiante ON inscripcion.`idestudiante` = estudiante.`idestudiante`
    INNER JOIN `instructor` instructor ON curso.`idinstructor` = instructor.`idinstructor`
    INNER JOIN `materia` materia ON curso.`idmateria` = materia.`idmateria`
WHERE
    curso.`idcurso` = $P{idcurso}
AND instructor.`visible` = 1
AND estudiante.`visible` = 1
AND instructor.`idinstructor` = $P{idinstructor}]]>
	</queryString>
